{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:webview:allow-create-webview",
    "core:webview:allow-set-webview-size",
    "core:window:allow-set-fullscreen"
  ]
}
